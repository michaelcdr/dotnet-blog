@model List<string>

<div class="card mb-3">
    <div class="card-body">
        <h3>Tags</h3>
        <div class="lista-tags">
            @foreach (var tag in Model)
            {
                <a asp-action="PorTag" asp-controller="Post" asp-route-tag="@tag">@tag</a>
            } 
        </div>
    </div>
</div>